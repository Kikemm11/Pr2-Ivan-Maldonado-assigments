<div id="<%= dom_id client %>">
  <p>
    <strong>Name:</strong>
    <%= client.name %>
  </p>

  <p>
    <strong>Legal entity:</strong>
    <%= client.legal_entity %>
  </p>

  <p>
    <strong>Doc number:</strong>
    <%= client.doc_number %>
  </p>

  <p>
    <strong>Telephone:</strong>
    <%= client.telephone %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= client.company.name %>
  </p>

  <p>
    <strong>Client code:</strong>
    <%= client.id %>
  </p>

</div>

<p style="color: green"><%= notice %></p>

<h1>Clients</h1>
<br>

<table class="table table-dark table-striped">
			<div id="clients">
	  <% @clients.each do |client| %>
		<tr>
		  	<td>
			    <%= render client %>
			   	<p>
			      <%= link_to "Show details of this state", client, class:"btn btn-secondary" %>
			    </p>
			 </td>
		 </tr>
	 	 <% end %>
		</div>
</table>

<%= link_to "Add new client", new_client_path, class:"btn btn-dark" %>
